    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "aistockpredictor",
    "privacy": "public",
    "description": "Get AI-powered stock predictions and real-time market signals. Make confident investment decisions with advanced analytics and sentiment analysis.",
    "primaryColor": "#2D8659",
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-font"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c0dee8dc-6f20-457f-9c6e-a5d5854a5894"
